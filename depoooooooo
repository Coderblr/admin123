{formErrors.acc_segment && (
  <div style={styles.fieldError}>{formErrors.acc_segment}</div>
)}



{formErrors.no_acc && (
  <div style={styles.fieldError}>{formErrors.no_acc}</div>
)}








{formErrors.cif && (
  <div style={styles.fieldError}>{formErrors.cif}</div>
)}



onChange={(e) => setCif(validation.formatMobile(e.target.value))}
